#include<bits/stdc++.h>
using namespace std;

int min(int arr[], int n)
{
    int min=arr[0];
    for(int i=1; i<n; i++)
    {
        if(min>arr[i])
        {
           min=arr[i];   
        }
    }
    
    return min;
}

int main()
{
    int arr[]={10, 89, 67, 56, 45, 78};
    int n = sizeof(arr)/sizeof(arr[0]);
    cout<<min(arr, n);
}
