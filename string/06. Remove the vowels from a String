#include<bits/stdc++.h>
using namespace std;

int length(string str)
{
   
    
    
   for(int i=0; str[i]!=0; i++)
   {
       
       if(str[i] == 'a' || str[i] == 'e' || str[i] == 'i' || str[i] == 'o' || str[i] == 'u' ||
          str[i] == 'A' || str[i] == 'E' || str[i] == 'I' || str[i] == 'O' || str[i] == 'U' )
       {
            continue;
       }
       else
       {
           cout<<str[i];
       }
       
   }
   
   return 0;
   
}

int main()
{
    string str;
    getline(cin, str);
    length(str);
}
