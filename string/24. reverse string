class Solution {
public:
    void reverseString(vector<char>& s) 
    {
        int l= s.size();
       int i,j;
        for(i=0 , j=l-1; i<j; i++,j--)
        {
            swap(s[i], s[j]);
        }

        for(int i=0; i<l; i++)
        {
            cout<<s[i];
        }
        
        
    }
};
