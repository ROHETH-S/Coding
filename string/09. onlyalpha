#include<bits/stdc++.h>
using namespace std;

int onlyalpha(string str)
{
    int l= str.size();

    for(int i=0; i<l; i++)
    {
        if((str[i]>= 'a' && str[i]<='z') || (str[i]>= 'A' && str[i]<='Z' ) )
        {
            cout<<str[i];
        }
        // if(isalpha(str[i]))
        // {
        //     cout<<str[i];
        // }
        else
        {
            continue;
        }
    }
    
    
    return 0;
   
}

int main()
{
    string str;
    getline(cin, str);
    onlyalpha(str);
}
