#include<bits/stdc++.h>
using namespace std;

int freq(string str)
{
    int l= str.size();
    unordered_map<char, int> mpp;
 
   for(int i=0; i<l; i++)
   {
       mpp[str[i]]++;
   }
    
   for(auto x: mpp)
   {
       cout<<x.first<<" "<<x.second<<endl;
   }
    return 0;
   
}

int main()
{
    string str;
    getline(cin, str);
   freq(str);
}
