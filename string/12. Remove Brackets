#include<bits/stdc++.h>
using namespace std;

int removebrackets(string str)
{
    int l= str.size();

    for(int i=0; i<l; i++)
    {
        if(str[i] == '('|| str[i]==')')
        {
            continue;
        }
        
        else
        {
            cout<<str[i];
        }
    }
    
    
    return 0;
   
}

int main()
{
    string str;
    getline(cin, str);
    removebrackets(str);
}

