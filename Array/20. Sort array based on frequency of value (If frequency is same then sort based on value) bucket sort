#include <bits/stdc++.h>
using namespace std;


void Frequency(int arr[], int N, int K)
{
    unordered_map<int, int> map;
 
    for (int i = 0; i < N; i++)
        map[arr[i]]++;
 
    // for (auto x : map)
    //     cout << x.first << " " << x.second << endl;
        
    vector<vector<int> > frequency(N + 1);
    for (auto x : map) 
    {
        frequency[x.second].push_back(x.first);
    }  
    
    // for(int i=0; i<frequency.size(); i++)
    // {
    //     for(int j=0; j<frequency[i].size(); j++)
    //     {
    //         cout<<frequency[i][j]<<" ";
    //     }
    // }
    
    int count = 0;
 
    for (int i = frequency.size() - 1; i >= 0; i--) 
    {
 
        
        if (frequency[i].size() > 1) 
        {
            sort(frequency[i].begin(), frequency[i].end(),
                 greater<int>());
        }
        
        for(int j=0; j<frequency[i].size(); j++)
        {
             if (frequency[i][j]<=K)
            {
                for (auto element : frequency[i]) 
                {
                   cout << element << " ";
                }
            }
            
        }
        
       
    }
    
}
 
int main()
{
    int arr[] = {1,1,1,1,2,2,4,4,4,7,7};
    int N = sizeof(arr) / sizeof(arr[0]);
    int K=2;
    Frequency(arr, N, K);
}
