class Solution {
public:
    int missingNumber(vector<int>& nums)
    {
        int l=nums.size();
        sort(nums.begin(), nums.end());
        int i;
        for(i=0; i<l; i++)
        {
            if(nums[i] != i)
            {
                break;
            }
        }
        
        return i;
    }
};
