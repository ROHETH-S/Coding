#include<bits/stdc++.h>
using namespace std;

vector<int> findmissing(vector<int> array)
{
    int n = array.size() + 1;

    vector<int> substitute(n, 0); // initializing the substitute array with 0 of size n+1.

    vector<int> ans;              // initializing the answer  array .

    for (int i = 0; i < array.size(); i++)
    {
        substitute[array[i]]++;
    }

    for (int i = 1; i <= array.size(); i++)
    {
        if (substitute[i] == 0)
        {
            ans.push_back(i);
        }
    }

    return ans;
}

int main()
{
    vector<int> array={3,0,1};
    vector<int> res=findmissing(array);
      for(auto & it: res)
    {
        cout<<it;
    }
    
}
