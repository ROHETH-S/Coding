#include<bits/stdc++.h>
using namespace std;

vector<int> rotatearray(vector<int>& nums, int k) 
{
    int l=nums.size();
    vector<int> temp(l);
   
    for(int i=0; i<l; i++)
    {
            temp[(i+k)%l]=nums[i];//right
            temp[(i - k + l) % l] = nums[i];
                            
    }
        
    nums=temp;
    
    return temp;
    
}

int main()
{
    vector<int> nums={1,2,3,4,5,6};
    int k=2;
    cout<<"rotatedarray:";
    vector<int> res=rotatearray(nums,k);
    for(auto & it: res)
    {
        cout<<it;
    }
 
}
